{
  "compilerOptions": {
    "target": "esnext",
    "module": "commonjs",
    "outDir": "./lib",
    "removeComments": false,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
  },
  "exclude": [
      "node_modules",
      "lib",
      "tst"
  ]
}
